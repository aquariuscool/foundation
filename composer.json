{
	"name": "orchestra/foundation",
	"description": "Orchestra Platform Foundation",
	"homepage": "http://orchestraplatform.com/docs/latest/",
	"license": "MIT",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "administrator", "admin", "user-management"],
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-4": {
			"Orchestra\\Foundation\\" : "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"require": {
		"php": ">=5.4.0",
		"laravel/framework": "5.0.*",
		"orchestra/asset": "3.0.*",
		"orchestra/auth": "3.0.*",
		"orchestra/extension": "3.0.*",
		"orchestra/html": "3.0.*",
		"orchestra/kernel": "3.0.*",
		"orchestra/memory": "3.0.*",
		"orchestra/messages": "3.0.*",
		"orchestra/model": "3.0.*",
		"orchestra/notifier": "3.0.*",
		"orchestra/optimize": "3.0.*",
		"orchestra/publisher": "3.0.*",
		"orchestra/support": "3.0.*",
		"orchestra/translation": "3.0.*",
		"orchestra/view": "3.0.*",
		"orchestra/widget": "3.0.*"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"orchestra/resources": "3.0.*",
		"orchestra/testing": "3.0.*",
		"twbs/bootstrap": "~3@stable"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	},
	"minimum-stability": "dev"
}
