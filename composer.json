{
    "name": "orchestra/foundation",
    "description": "Orchestra Platform Foundation",
    "homepage": "http://orchestraplatform.com/docs/latest/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "administrator", "admin", "user-management"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Foundation\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "~5.0.35",
        "orchestra/asset": "~3.0.0",
        "orchestra/auth": "~3.0.2",
        "orchestra/extension": "~3.0.2",
        "orchestra/html": "~3.0.2",
        "orchestra/kernel": "~3.0.4",
        "orchestra/memory": "~3.0.6",
        "orchestra/messages": "~3.0.0",
        "orchestra/model": "~3.0.0",
        "orchestra/notifier": "~3.0.3",
        "orchestra/optimize": "~3.0.3",
        "orchestra/publisher": "~3.0.1",
        "orchestra/support": "~3.0.4",
        "orchestra/translation": "~3.0.1",
        "orchestra/view": "~3.0.1",
        "orchestra/widget": "~3.0.0",
        "predis/predis": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.1",
        "orchestra/resources": "~3.0.0",
        "orchestra/testing": "~3.0.3"
    },
    "suggest": {
        "orchestra/installer": "Allow Orchestra Platform to be installed in new application (~3.0).",
        "orchestra/studio": "Allow additional code generator for Orchestra Platform (~3.0).",
        "orchestra/testing": "Allow better unit testing support for Orchestra Platform (~3.0)."
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ],
        "update:frontend": [
            "bower update",
            "bower-installer",
            "gulp"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev"
}
